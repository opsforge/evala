{
  "name": "evala-opsforge",
  "version": "2.2.1",
  "description": "Evala customised for ANViL",
  "main": "src/server.js",
  "scripts": {
    "build-static": "cpx \"src/static-files/**/*.*\" build",
    "watch-static": "cpx \"src/static-files/**/*.*\" build --watch",
    "build-app": "browserify ./src/app/App.js -o ./build/app.js -t babelify && uglifyjs ./build/app.js -o ./build/app.js -c -m",
    "watch-app": "watchify ./src/app/App.js -o ./build/app.js -t babelify -v",
    "build": "rm -rf ./build/* && npm run build-app && npm run build-static && rm ./packages/* && node ./scripts/zipExtension.js",
    "watch": "rm -rf ./build/* && concurrently \"npm run watch-app\" \"npm run watch-static\"",
    "test": "jest",
    "start": "node ./src/server.js --shell=$SHELL"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opsforge/evala.git"
  },
  "author": "Krasimir Tsonev / George Svachulay",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chromath": "^0.0.7",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "kuker-emitters": "6.7.3",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-iframe": "^1.2.0",
    "regenerator-runtime": "^0.11.1",
    "stent": "5.0.2",
    "uglify-js": "^3.3.9",
    "watchify": "^3.9.0",
    "xterm": "^3.0.2",
    "zip-folder": "^1.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "express-ws": "^3.0.0",
    "yargs": "^11.0.0",
    "node-pty": "^0.7.4"
  },
  "bin": {
    "evala": "./src/server.js"
  },
  "bugs": {
    "url": "https://github.com/opsforge/evala/issues"
  },
  "homepage": "https://github.com/opsforge/evala#readme",
  "keywords": []
}
